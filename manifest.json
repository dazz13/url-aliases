{
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "Alt+A"
      }
    }
  },
  "description": "URL Aliases is a Chrome extension that allows users to create aliases (short strings) for website URLs.",
  "host_permissions": [ "https://www.google.com/search?q=*" ],
  "icons": {
    "128": "/icons/128.png",
    "16": "/icons/16.png",
    "48": "/icons/48.png"
  },
  "manifest_version": 3,
  "name": "URL Aliases",
  "permissions": [ "declarativeNetRequest", "tabs", "downloads" ],
  "version": "1.72"
}
